\def\Gradb#1{\Grad[{#1}]}         % Gradient boxed Argument Grad[#]
\def\rot{\operatorname{rot}}      % rot ohne Argument
\def\Rot{\operatorname{Rot}}      % Rot ohne Argument
\def\rotb#1{\rot[{#1}]}           % rot boxed Argument rot[#]
\def\Rotb#1{\Rot[{#1}]}           % Rot boxed Argument Rot[#]
