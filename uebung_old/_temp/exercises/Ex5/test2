feap * * Zugstab
0 0 1 2 2 3
!numnp numel nummat ndm ndf nen

parameter
 n  = 10        ! Number of elements at boundaries
 lx = 4         ! dimension in x-direction
 ly = 4         ! dimension in y-direction
 p0 = 1000.0    ! Stress at the right boundary
 pp = p0*ly/n/2 ! Load per node in each element

bloc
 cartesian n n 1 1 1 0 5
 1 0. 0.
 2 lx 0.
 3 lx ly
 4 0. ly

boun
 1        n+1   -1 0
(n+1)*n+1 0      1 0

eboun
 2 0.0    0 1

eforc
 1 lx     2*pp 0.0

forc
         n+1 0 -pp 0.0
 (n+1)*(n+1) 0 -pp 0.0

mate 1
 user,3
 210000.0   0.3
!young-mod  nue

end

inter

stop
